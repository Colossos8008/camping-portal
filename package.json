{
  "name": "camping-portal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",

    "build": "next build",
    "vercel-build": "prisma migrate deploy && next build",

    "start": "next start",
    "lint": "eslint",

    "prisma:generate": "dotenv -e .env -- prisma generate",
    "db:migrate": "dotenv -e .env -- prisma migrate dev",
    "db:deploy": "dotenv -e .env -- prisma migrate deploy",

    "db:push": "node scripts/blocked-db-push.mjs",

    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@supabase/supabase-js": "^2.93.3",
    "dotenv": "^17.2.3",
    "leaflet": "^1.9.4",
    "next": "16.1.4",
    "next-themes": "^0.4.6",
    "pg": "^8.17.2",
    "prisma": "^7.3.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-leaflet": "^5.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
